{
  "expo": {
    "name": " ",
    "slug": "rds-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/rds-logo-white.jpg",
    "backgroundColor": "green",
    "scheme": "rdsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rds.mobile",
      "infoPlist": {
        "NSBonjourServices": [
          "_tcp.local.",
          "_tcp.local."
        ],
        "NSLocalNetworkUsageDescription": "Allow RDS Screen to discover devices on the local network.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.networking.multicast": true
      }
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.rdsconnect.com",
              "pathPrefix": "/reset-password"
            },
            {
              "scheme": "rdsconnect"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/rds-logo-black.png",
        "backgroundColor": "#0A192F"
      },
      "permissions": [
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CHANGE_WIFI_MULTICAST_STATE",
        "android.permission.INTERNET"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.rdsconnect.rdsapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A192F"
        }
      ],
      "expo-video",
      "expo-font",
      "expo-web-browser",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "00075f18-0ac0-4335-bae1-51ec1923cf81"
      }
    },
    "owner": "rdsconnect"
  }
}
